{
  "manifest_version": 3,
  "version": "1.0",
  "name": "main-world-extension",
  "description": "An extension running in the main world.",
  "background": {
    "service_worker": "./dist/background.js"
  },
  "content_scripts": [
    {
      "matches": [
        "https://extension.js.org/*"
      ],
      "js": [
        "./dist/content.js"
      ]
    }
  ]
}